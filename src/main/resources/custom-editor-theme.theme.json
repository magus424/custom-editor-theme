
{
  "name": "Custom Editor Theme",
  "dark": true,
  "author": "magus424",
  "editorScheme": "/custom-editor-theme.xml",
  "colors": {
    "backgroundColor": "#011627",
    "contrastColor": "#010E1A",
    "foregroundColor": "#D6DEEB",
    "textColor": "#5F7E97",
    "secondaryTextColor": "#5F7E97",
    "selectionBackgroundColor": "#5f7e9790",
    "selectionForegroundColor": "#FFFFFF",
    "buttonColor": "#0B253A",
    "secondaryBackgroundColor": "#0B2942",
    "disabledColor": "#697098",
    "selectedTextColor": "#13344F",
    "tableSelectedColor": "#13344F",
    "treeSelectionColor": "#13344f50",
    "separatorsColor": "#122D42",
    "highlightColor": "#084D81",
    "notificationColor": "#01111D",
    "accentColor": "#0072BC",
    "excludedFilesColor": "#0E293F",
    "white": "#FFFFFF",
    "blue": "tableSelectedColor",
    "green": "#024C2B",
    "orange": "#4C2600",
    "red": "#4C0000",
    "violet": "#4C004C",
    "yellow": "#4C4C00",
    "temp": "#071F34"
  },
  "ui": {
    "*": {
      "foreground": "foregroundColor",
      "textForeground": "foregroundColor",
      "caretForeground": "foregroundColor",
      "selectionForeground": "selectionForegroundColor",
      "selectionForegroundInactive": "selectionForegroundColor",
      "selectionInactiveForeground": "selectionForegroundColor",
      "background": "backgroundColor",
      "textBackground": "backgroundColor",
      "inactiveBackground": "backgroundColor",
      "selectionBackground": "selectionBackgroundColor",
      "treeSelection": "treeSelectionColor",
      "disabledForeground": "disabledColor",
      "disabledText": "disabledColor"
    },
    "ActionButton": {
      "hoverBackground": "highlightColor",
      "hoverBorderColor": "highlightColor",
      "pressedBackground": "accentColor",
      "pressedBorderColor": "accentColor"
    },
    "Borders": {
      "color": "separatorsColor",
      "ContrastBorderColor": "backgroundColor"
    },
    "Button": {
      "arc": 5,
      "foreground": "secondaryTextColor",
      "disabledText": "disabledColor",
      "disabledBorderColor": "buttonColor",
      "startBackground": "buttonColor",
      "endBackground": "buttonColor",
      "startBorderColor": "buttonColor",
      "endBorderColor": "buttonColor",
      "shadowWidth": 0,
      "default": {
        "foreground": "white",
        "startBackground": "secondaryBackgroundColor",
        "endBackground": "secondaryBackgroundColor",
        "startBorderColor": "secondaryBackgroundColor",
        "endBorderColor": "secondaryBackgroundColor"
      }
    },
    "CheckBox": {
      "select": "accentColor"
    },
    "ComboBox": {
      "ArrowButton": {
        "background": "backgroundColor",
        "nonEditableBackground": "backgroundColor",
        "iconColor": "white"
      },
      "disabledBackground": "backgroundColor",
      "disabledForeground": "disabledColor",
      "nonEditableBackground": "backgroundColor"
    },
    "CompletionPopup": {
      "foreground": "white",
      "background": "backgroundColor",
      "infoForeground": "secondaryTextColor",
      "nonFocusedState": false
    },
    "Component": {
      "arc": 0,
      "borderColor": "separatorsColor",
      "disabledBorderColor": "separatorsColor",
      "infoForeground": "secondaryTextColor",
      "focusedBorderColor": "accentColor",
      "focusWidth": 1
    },
    "DefaultTabs": {
      "hoverBackground": "highlightColor",
      "underlineColor": "accentColor",
      "inactiveUnderlineColor": "accentColor",
      "underlinedTabBackground": "tableSelectedColor",
      "underlinedTabForeground": "white"
    },
    "Editor": {
      "background": "backgroundColor"
    },
    "EditorTabs": {
      "selectedBackground": "tableSelectedColor",
      "underlinedTabBackground": "tableSelectedColor",
      "underlinedTabForeground": "white"
    },
    "FileColor": {
      "Blue": "selectedTextColor",
      "Green": "green",
      "Orange": "orange",
      "Rose": "red",
      "Violet": "violet",
      "Yellow": "yellow"
    },
    "Label": {
      "infoForeground": "secondaryTextColor",
      "selectedForeground": "selectionForeground"
    },
    "Link": {
      "activeForeground": "accentColor"
    },
    "List": {
      "selectionBackground": "tableSelectedColor"
    },
    "Menu": {
      "borderColor": "backgroundColor",
      "acceleratorForeground": "secondaryTextColor",
      "acceleratorSelectionForeground": "secondaryTextColor",
      "disabledForeground": "secondaryTextColor"
    },
    "MenuBar": {
      "borderColor": "backgroundColor"
    },
    "MenuItem": {
      "acceleratorForeground": "secondaryTextColor",
      "acceleratorSelectionForeground": "secondaryTextColor",
      "disabledForeground": "disabledColor"
    },
    "NavBar": {
      "borderColor": "backgroundColor"
    },
    "Notification": {
      "errorBackground": "red",
      "errorBorderColor": "red"
    },
    "Plugins": {
      "hoverBackground": "treeSelectionColor",
      "lightSelectionBackground": "tableSelectedColor",
      "disabledForeground": "white",
      "Button": {
        "installBackground": "buttonColor",
        "installBorderColor": "buttonColor",
        "installForeground": "white",
        "installFillBackground": "buttonColor",
        "installFillForeground": "white",
        "updateBackground": "buttonColor",
        "updateBorderColor": "buttonColor",
        "updateForeground": "white"
      }
    },
    "Popup": {
      "borderColor": "backgroundColor",
      "inactiveBorderColor": "backgroundColor",
      "paintBorder": true,
      "Header": {
        "activeBackground": "backgroundColor",
        "inactiveBackground": "contrastColor"
      },
      "Toolbar": {
        "background": "backgroundColor",
        "borderColor": "backgroundColor"
      }
    },
    "ProgressBar": {
      "trackColor": "highlightColor",
      "progressColor": "accentColor",
      "indeterminateStartColor": "accentColor",
      "indeterminateEndColor": "#00FFFF"
    },
    "SearchEverywhere": {
      "SearchField": {
        "background": "contrastColor",
        "borderColor": "backgroundColor",
        "infoForeground": "white"
      },
      "Tab": {
        "selectedForeground": "selectionForegroundColor",
        "selectedBackground": "accentColor"
      }
    },
    "SearchMatch": {
      "startBackground": "accentColor",
      "endBackground": "accentColor"
    },
    "Separator": {
      "separatorColor": "separatorsColor"
    },
    "Slider": {
      "trackColor": "highlightColor",
      "tickColor": "highlightColor",
      "buttonColor": "accentColor",
      "buttonBorderColor": "accentColor"
    },
    "StatusBar": {
      "borderColor": "separatorsColor"
    },
    "TabbedPane": {
      "hoverColor": "highlightColor"
    },
    "Table": {
      "hoverBackground": "highlightColor",
      "selectionBackground": "tableSelectedColor"
    },
    "TextArea": {
      "background": "backgroundColor"
    },
    "TextField": {
      "background": "backgroundColor",
      "disabledBackground": "backgroundColor",
      "inactiveForeground": "disabledColor"
    },
    "TitlePane": {
      "background": "contrastColor",
      "infoForeground": "secondaryTextColor",
      "inactiveInfoForeground": "secondaryTextColor",
      "inactiveBackground": "backgroundColor"
    },
    "ToolTip": {
      "borderColor": "backgroundColor"
    },
    "ToolWindow": {
      "Header": {
        "background": "backgroundColor",
        "inactiveBackground": "backgroundColor"
      },
      "HeaderTab": {
        "underlinedTabBackground": "selectedTextColor"
      }
    },
    "Tree": {
      "foreground": "secondaryTextColor",
      "selectionInactiveForeground": "secondaryTextColor",
      "selectionBackground": "treeSelectionColor"
    },
    "VersionControl": {
      "Log.Commit": {
        "currentBranchBackground": "#2E3D5A",
        "unmatchedForeground": "#727380"
      },
      "FileHistory.Commit.selectedBranchBackground": "#2E3D5A"
    }
  },
  "icons": {
    "ColorPalette": {
      "Checkbox.Background.Default": "#010E1A",
      "Checkbox.Background.Default.Dark": "#010E1A",
      "Checkbox.Background.Disabled": "#011627",
      "Checkbox.Background.Disabled.Dark": "#011627",
      "Checkbox.Border.Default": "#084D81",
      "Checkbox.Border.Default.Dark": "#084D81",
      "Checkbox.Border.Disabled": "#084D8170",
      "Checkbox.Border.Disabled.Dark": "#084D8170",
      "Checkbox.Border.Selected": "#0072BC",
      "Checkbox.Border.Selected.Dark": "#0072BC",
      "Checkbox.Foreground.Selected": "#0072BC",
      "Checkbox.Foreground.Selected.Dark": "#0072BC",
      "Checkbox.Focus.Thin.Default": "#084D81",
      "Checkbox.Focus.Thin.Default.Dark": "#084D81",
      "Checkbox.Focus.Thin.Selected": "#0072BC",
      "Checkbox.Focus.Thin.Selected.Dark": "#0072BC",
      "Checkbox.Focus.Wide": "#084D81",
      "Checkbox.Focus.Wide.Dark": "#084D81"
    }
  }
}